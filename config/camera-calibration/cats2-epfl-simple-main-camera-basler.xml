<?xml version="1.0"?>
<!-- 
	The (0,0) is in the bottom left corner of the aquarium.
	At the moment only five points are used for the test purposes.
	The world distances are set in mm, because when set in meters
	the calibration fails (due to float used?)
 -->
<opencv_storage>
    <cameraType>basler</cameraType>
    <worldUnits>mm</worldUnits>
    <agentHeight>30</agentHeight>
    <imageSize>
        <width>2048</width>
        <height>2048</height>
    </imageSize>
    <calibrationPoints>
        <numberOfPoints>5</numberOfPoints>
        <point_0>
            <xWorld>0</xWorld>
            <yWorld>0</yWorld>
            <xImage>183</xImage>
            <yImage>666</yImage>
        </point_0>
        <point_1>
            <xWorld>0</xWorld>
            <yWorld>980</yWorld>
            <xImage>174</xImage>
            <yImage>47</yImage>
        </point_1>
        <point_2>
            <xWorld>980</xWorld>
            <yWorld>0</yWorld>
            <xImage>796</xImage>
            <yImage>660</yImage>
        </point_2>
        <point_3>
            <xWorld>980</xWorld>
            <yWorld>980</yWorld>
            <xImage>799</xImage>
            <yImage>44</yImage>
        </point_3>
        <point_4>
            <xWorld>490</xWorld>
            <yWorld>490</yWorld>
            <xImage>489</xImage>
            <yImage>357</yImage>
        </point_4>
    </calibrationPoints>
</opencv_storage>

